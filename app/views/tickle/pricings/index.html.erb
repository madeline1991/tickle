<h1>Taco Insurance Company of Kentucky and Lower Europe</h1>

<form action="<%= pricings_url %>" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

  <label>Age</label>
  <input type="number" name="pricings[age]" value="">

  <label>Gender</label>
  <select type="text" name="pricings[gender]">
    <option value="Male">Male</option>
    <option value="Female">Female</option>
  </select>

  <label>Allergies</label>
  <input type="checkbox" name="pricings[conditions][]" value="Allergies">

  <label>Sleep Apnea</label>
  <input type="checkbox" name="pricings[conditions][]" value="Sleep Apnea">

  <label>Heart Disease</label>
  <input type="checkbox" name="pricings[conditions][]" value="Heart Disease">
  <input type="submit" value="submit">
<form>
